SELECT
advertiser_id,
 advertiser_name,
 insertion_order_id,
 insertion_order_name,
 package_id,
 package_name,
 line_item_id,
line_item_name,
 ad_id,
 creative_id,
 creative_name,
SUM(to_currency(cost,'aud',impression_date)) as cost_aud,
(to_market_date(impression_datetime, 'yyyy-MM-dd')) as impression_datetime,
 time_zone_offset,
 mobile_device_id,
inventory_source_name,
 ad_format_id,
 creative_size,
video_player_size as video_player_size_duplicate,
 video_playback_method as video_playback_method_duplicate,
video_linear_type_id as video_linear_type_id_duplicate,
 tld,
 subdomain,
 subdomain as subdomain_level2,
 region_name,
 dma_name,
city_name,
 postal_code,
 country_name,
 device_type,
mobile_device_type_id,
  os_type,
case when os_type=0 then 'Unknown'
 when os_type=1 then 'Windows'
 when os_type=2 then 'Linux'
 when os_type=3 then 'Mac OS'
 when os_type=4 then 'iOS (iPad)'
 when os_type=5 then 'iOS (iPhone)'
 when os_type=6 then 'Android'
else 'Unknown' end os_type_name,
 os_version,
 browser_type,
 browser_type_name,
net_connection_speed,
 media_channel_name,
 format,
 environment,
 app_id,
 app_name,
 first_attribution_contract_id,
second_attribution_contract_id,
 third_attribution_contract_id,
