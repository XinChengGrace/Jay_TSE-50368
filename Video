content_context,
 content_episode,
 content_genre,
 content_id,
 content_isrc,
 content_language,
 content_length,
content_live_stream,
 content_production_quality,
 content_qag_media_rating,
 content_rating,
 content_season,
content_series,
 content_title,
 content_url,
 engagement_event_id,
 engagement_event_ts,
 exchange_publisher_name,
gross_video_ad_call,
 resume_events,
 rewind_events,
 skip_events,
 video_linear_type_id,
 video_max_duration,
video_min_duration,
 video_placement,
 video_placement_id,
video_playback_method,
 video_player_height,
 video_player_size,
 video_player_width,
video_skippability_id,
 view_event_measurement_id,
 view_event_platform_id,
 view_events,
 acc_100pct_vis_aud_sec,
acc_100pct_vis_sec,
 acc_50pct_vis_aud_sec,
 acc_50pct_vis_sec,
 acc_aud_sec,
 analyzed_imp,
 avoc_imp,
max_cnt_50pct_vis_aud_sec,
 max_cnt_50pct_vis_sec,
 max_cnt_aud_sec,
 measurable_imp,
 pause_events,
 percent25_events,
percent50_events,
 percent75_events,
 viewability_video_duration,
 viewability_video_height,
 viewability_video_width
FROM amobee_media 
 where market_id = 301 
 and date > '2022_09_04'
 and event_type in('impression')
 group by 
advertiser_id,
 advertiser_name,
 insertion_order_id,
 insertion_order_name,
 package_id,
 package_name,
 line_item_id,
